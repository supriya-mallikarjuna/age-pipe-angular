{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "age": {
      "projectType": "library",
      "root": "projects/age",
      "sourceRoot": "projects/age/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/age/tsconfig.lib.json",
            "project": "projects/age/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/age/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/age/src/test.ts",
            "tsConfig": "projects/age/tsconfig.spec.json",
            "karmaConfig": "projects/age/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/age/tsconfig.lib.json",
              "projects/age/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "1c4103a9-ae97-475c-b187-eab097ea7db4"
  },
  "defaultProject": "age"
}